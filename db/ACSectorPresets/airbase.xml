    <description>HUD_F1_SECTOR_DESCRIPTION_COMBINED_3</description>
    <sectorIconPath>icons/hud/sectors/sectorType{size}Airfield2_{team}</sectorIconPath>
    <featuresIconPath>icons/hud/sectors/sectorIcon{size}RespawnChoice_{team}</featuresIconPath>
